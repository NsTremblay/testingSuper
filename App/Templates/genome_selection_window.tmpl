<!-- List, Tree and Map Panels for Genome Selection -->
<div class="panel-group genome-search" id="acoridian">

	<!-- Collapsible table panel -->
	<div class="panel panel-default">
		<div class="panel-heading" id="table-panel-header">
			<h4 class="panel-title">
				<a data-toggle="collapse" data-parent="#accordian" href="#table-panel">
					Genome Table
				</a>
			</h4>
		</div>
		<div id="table-panel" class="panel-collapse collapse in">
			<div class="panel-body">
				<div id="strains_table"></div>
			</div>
		</div>
	</div>
	<!-- end of table panel -->

	<!-- Collapsible tree panel -->
	<div class="panel panel-default">
		<div class="panel-heading" id="tree-panel-header">
			<h4 class="panel-title">
				<a data-toggle="collapse" data-parent="#accordian" href="#tree-panel">
					Phylogenetic Tree
				</a>
			</h4>
		</div>
		<div id="tree-panel" class="panel-collapse collapse in">
			<div class="panel-body">
				<div id="strains_tree"></div>
			</div>
		</div>
	</div>
	<!-- end of tree panel -->

	<!-- Collapsible map panel -->
	<div class="panel panel-default">
		<div class="panel-heading" id="map-panel-header">
			<h4 class="panel-title">
				<a data-toggle="collapse" data-parent="#accordian" href="#map-panel">
					Geospatial Search
				</a>
			</h4>
		</div>
		<div id="map-panel" class="panel-collapse collapse in">
			<div class="panel-body">
				<div id="strains_map"></div>
			</div>
		</div>
	</div>
	<!-- end of map panel -->

</div>