<script>
//Left sidebar
$("#sidebar-expand").click(function(e) {
	e.preventDefault();
	$("#wrapper").toggleClass("active");
	$(this).hide();
	$("#sidebar-collapse").show();
});
$("#sidebar-collapse").click(function(e) {
	e.preventDefault();
	$("#wrapper").toggleClass("active");
	$("#sidebar-expand").show();
	$(this).hide();
});
//Right sidebar
$("#sidebar-right-expand").click(function(e) {
	e.preventDefault();
	$("#wrapper").toggleClass("right-active");
	$("#page-content-wrapper").toggleClass("right-inactive");
	$(this).hide();
	$("#sidebar-right-collapse").show();
});
$("#sidebar-right-collapse").click(function(e) {
	e.preventDefault();
	$("#wrapper").toggleClass("right-active");
	$("#page-content-wrapper").toggleClass("right-inactive");
	$("#sidebar-right-expand").show();
	$(this).hide();
});
</script>

