<div class="span4">
	<form id="mSSearchForm" class="form" style="padding:10px 0px 5px 0px" action="/home/dispatch.cgi/multi_strain/" method="post" enctype="application/x-www-form-urlencoded">
		<fieldset>
			<h5 style="margin:0px 0px 0px 10px">Select multiple strains to compare them and view metadata</h5>
			<div style="margin:5px;height:140px;overflow:auto;border-style:solid;border-width:1px;border-color:#d3d3d3" class="row-fluid">
				<TMPL_LOOP NAME=FEATURES>
				<label class="checkbox">
					<input type="checkbox" value='<TMPL_VAR NAME=UNIQUENAME>' name="multiStrainNames"><TMPL_VAR NAME=UNIQUENAME></input>
				</label>
				</TMPL_LOOP>
			</div>
			<div style="padding:5px 0px 0px 10px" class="row-fluid">
				<button class="btn btn-primary btn-small"><i class="icon-plus-sign icon-white"></i> Add to Group 1</button>
				<button class="btn btn-primary btn-small"><i class="icon-plus-sign icon-white"></i> Add to Group 2</button>
				<button type="reset" class="btn btn-small btn-danger" name="Cancel" value="Cancel"><i class="icon-remove icon-white"></i> Cancel</button>
				<div style="margin:10px 10px 10px 0px">
					<button type="submit" class="btn btn-primary btn-small span12" value="Submit" onclick="checkMSFields(this.form)"><i class="icon-ok icon-white"></i> Compare Strains</button>
				</div>
			</div>
		</fieldset>
	</form>

	<div id="group1" class="span12" style="padding:0px 0px 10px 0px">
		<form style="border:solid;border-width:1px;padding:10px">
			<span class="help-block">Group 1: </span>
			<fieldset>
			</fieldset>
		</form>
	</div>

	<div id="group2" class="span12" style="padding:0px 0px 10px 0px">
		<form style="border:solid;border-width:1px;padding:10px">
			<span class="help-block">Group 2: </span>
			<fieldset>
			</fieldset>
		</form>
	</div>

	<script type="text/javascript">
	function checkMSFields(mSSearchForm) {
		var selected = 0;
		var i = 0;
		while (i < document.getElementById('mSSearchForm').getElementsByTagName('input').length){
			if (document.getElementById('mSSearchForm').getElementsByTagName('input')[i].checked == true) {
				selected++;
				i++;
			}
			else {
				i++;
			}
		}
		if (selected == 0) {
			alert("You must make a valid selection");
			return false;
		}
		else {
			return true;
		}
	}
	</script>
</div>